<!DOCTYPE html>
<html>
<head>
  <title>Commander</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= raw show_flash_messages %>
	<div class="container-fluid">
	  <div class="row-fluid header-sensor">
	  	<div class="span12">
	  		<div class="page-header">
	  			<h1>&nbsp;GINA Sensor<small style="color: #ffffff;"> Commander</small></h1>
	  			<div class="icon-sensor">
	  				<table>
	  		  	  <tr>
	  		  	  	<td><i class="icon-time icon-white"></i></td>
	  		  	    <td><i class="icon-cog icon-white"></i></td>
	  		  	    <td><i class="icon-tint icon-white"></i></td>
	  		    	  <td><i class="icon-screenshot icon-white"></i></td>
	  		    	</tr>
	  		    </table>
	  		  </div>
	  		</div>
	  	</div>
	  </div>
	  <div class="row-fluid">
	  	<div class="span12">
	  		<ul class="nav nav-tabs">
	  			<li><a href="/">Home</a></li>
	  			<li><a href="/platforms">Platforms</a></li>
	  		</ul>
	  	</div>
	  </div>
	  <div class="row-fluid">
	  	<div class="span12">
	      <%= yield %>
	  	</div>
	  </div>
	  <div class="row-fluid footer-sensor">
	  	<div class="span12">
	  		Footer here
    	</div>
    </div>
  </div>
</body>
</html>
